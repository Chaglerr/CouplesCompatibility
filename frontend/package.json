{
  "name": "driwo-washing",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "gts lint && prettier src --check --cache",
    "preview": "vite preview",
    "fix": "gts fix && prettier src --write --cache | grep -v \"unchanged\"",
    "format": "gts fix && prettier src --write --cache | grep -v \"unchanged\"",
    "clean": "gts clean",
    "compile": "tsc -b",
    "prepare": "npm run compile",
    "pretest": "npm run compile",
    "posttest": "npm run lint",
    "theme": "sass .:. --no-source-map",
    "theme-watch": "sass .:. --watch --no-source-map"
  },
  "dependencies": {
    "@formatjs/intl-datetimeformat": "^6.18.0",
    "@formatjs/intl-numberformat": "^8.15.4",
    "axios": "^1.7.7",
    "gts": "^5.3.1",
    "lodash": "^4.17.21",
    "mobx": "^6.13.1",
    "mobx-react-lite": "^4.0.7",
    "primeflex": "^3.3.1",
    "primeicons": "^7.0.0",
    "primereact": "^10.8.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-intl": "^6.6.8",
    "react-leaflet": "^4.2.1",
    "react-router": "^6.26.1",
    "react-router-dom": "^6.26.1",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.22.0",
    "@types/lodash": "^4.17.9",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^9.22.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "gts": "^5.3.1",
    "prettier": "^3.3.3",
    "sass": "~1.78.0",
    "sass-watch": "^0.1.0",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.26.1",
    "vite": "^6.3.1"
  }
